<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Its.Configuration.Settings.Precedence" value="ExampleDevelopment" />
  </appSettings>
  <runtime>
    <!--loadFromRemoteSources is necessary when the assemblies being loaded are in different child directories of the root.-->
    <loadFromRemoteSources enabled="false" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>